{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "checkJs": false,
    "allowJs": false,
    "moduleResolution": "node",
    "removeComments": true,
    "noEmitOnError": true,
    "pretty": true
  }
}
